{
  "manifest_version": 2,
  "name": "LJ Style Mine",
  "version": "1.0",

  "description": "LiveJournal style mine.",
  "icons" : {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
